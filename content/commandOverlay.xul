  <!-- QUERY CONTEXT MENU -->
  <menupopup id="bookmarktags:queryContextMenu"
             onpopupshowing="BookmarkTags.QueryCmds.update();">
    <menuitem command="bookmarktags:queryCmds:removeSucceedingTags"
              default="true" />
    <menuitem command="bookmarktags:queryCmds:removeTag" />
    <menuitem command="bookmarktags:queryCmds:removeOtherTags" />
    <menuseparator />
    <menuitem command="bookmarktags:queryCmds:removeAllTags" />
    <menuseparator />
    <menuitem command="bookmarktags:queryCmds:customize" />
    <menuitem command="bookmarktags:queryCmds:help" />
  </menupopup>


  <!-- QUERY INPUT CONTEXT MENU -->
  <menupopup id="bookmarktags:queryInputContextMenu"
             onpopupshowing="try { document.commandDispatcher.focusedElement.parentNode._doPopupItemEnabling(this); } catch(e) { return false; } BookmarkTags.TagInputCmds.update(); BookmarkTags.QueryCmds.update();">
    <menuitem label="&undoCmd.label;"
              accesskey="&undoCmd.accesskey;"
              oncommand="goDoCommand('cmd_undo');"
              cmd="cmd_undo" />
    <menuseparator />
    <menuitem label="&cutCmd.label;"
              accesskey="&cutCmd.accesskey;"
              oncommand="goDoCommand('cmd_cut');"
              cmd="cmd_cut" />
    <menuitem label="&copyCmd.label;"
              accesskey="&copyCmd.accesskey;"
              oncommand="goDoCommand('cmd_copy');"
              cmd="cmd_copy" />
    <menuitem label="&pasteCmd.label;"
              accesskey="&pasteCmd.accesskey;"
              oncommand="goDoCommand('cmd_paste');"
