              oncommand="goDoCommand('cmd_selectAll');"
              cmd="cmd_selectAll" />
    <menuseparator />
    <menuitem command="bookmarktags:tagInputCmds:toggleAutocomplete"
              type="checkbox"
              autocheck="false" />
    <menuitem command="bookmarktags:queryCmds:toggleFindAsYouType"
              type="checkbox"
              autocheck="false" />
    <menuseparator />
    <menuitem command="bookmarktags:tagInputCmds:help" />
